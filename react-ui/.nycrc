{
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "include": [
        "src/**/*.js"
    ],
    "exclude": [
        "src/index.js"
    ],
    "reporter": [
        "lcov",
        "text-summary"
    ],
    "cache": true,
    "all": true,
    "check-coverage": true,
    "report-dir": "./reports/test/unit/coverage"
}